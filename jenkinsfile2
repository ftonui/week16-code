pipeline {
    agent any

    stages {
        stage('sonarqube scan') {
            steps {
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=ftonui_week16-code'
        }
        stage('all maven command'){
            steps{
                sh 'mvn clean test compile install package'
            }
        }
    }
    }
}
    
    

    